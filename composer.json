{
    "name": "penpencilmarkerpen/adf-to-gfm",
    "description": "Atlassian Document Format (ADF) to GitHub Flavored Markdown (GFM) converter",
    "keywords": [
        "penpencilmarkerpen",
        "adf-to-gfm"
    ],
    "homepage": "https://github.com/PenPencilMarkerPen/adf-to-gfm",
    "license": "MIT",
    "authors": [
        {
            "name": "Fork PenPencilMarkerPen",
            "email": "penpencilmarkerpen@google.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28",
        "symfony/var-dumper": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Karvaka\\AdfToGfm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Karvaka\\AdfToGfm\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
